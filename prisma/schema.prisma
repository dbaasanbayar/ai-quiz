generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model articles {
  id      Int       @id @default(autoincrement())
  article String?   @db.VarChar
  content String?   @db.VarChar
  date    DateTime? @db.Timestamp(6)
}

model saverecord {
  id      Int       @id @default(autoincrement())
  article String?   @db.VarChar
  content String?   @db.VarChar
  date    DateTime? @db.Timestamp(6)
}